package tablademult;
import java.util.Scanner;
/**
 *
 * @author loren
 */
public class TabladeMult {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner sc = new Scanner (System.in);
        
        int nro;
        int lim;
        System.out.println("Ingrese el numero de tabla que desea conocer y hasta que numero");
        nro = sc.nextInt();
        lim = sc.nextInt();
        
        int mult;
        int c=1;
        do {
            mult = (nro * c);
            System.out.println(nro + " x " + c + " = " + mult);
            c++;
        } while (c <= lim );
        
    }
    
}
